  `work_name` text COLLATE utf8_unicode_ci NOT NULL,
  `position_name` text COLLATE utf8_unicode_ci NOT NULL,
  `phone_number` text COLLATE utf8_unicode_ci NOT NULL,
  `email` text COLLATE utf8_unicode_ci NOT NULL,
  `presubject_id` int(11) NOT NULL,
  `lec_1` blob NOT NULL,
  `lec_2` blob NOT NULL,
  `lec_3` blob NOT NULL,
  `lec_4` blob NOT NULL,
