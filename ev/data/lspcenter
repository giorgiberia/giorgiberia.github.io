  `current_activity` blob NOT NULL,
  `middle_exam` blob NOT NULL,
  `final_exam` blob NOT NULL,
  `basiclit` blob NOT NULL,
  `addtionallit` blob NOT NULL,
  `code` text COLLATE utf8_unicode_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci KEY_BLOCK_SIZE=8 ROW_FORMAT=COMPACT;

